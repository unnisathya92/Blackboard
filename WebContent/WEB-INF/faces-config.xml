<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
	version="2.2">
	<navigation-rule>
		<from-view-id>/jsp/login.jsp</from-view-id>
		<navigation-case>
			<from-outcome>Student</from-outcome>
			<to-view-id>/jsp/StudentHome.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>Teacher</from-outcome>
			<to-view-id>/jsp/TeacherHome.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>Admin</from-outcome>
			<to-view-id>/jsp/dbAccess.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/jsp/StudentHome.jsp</from-view-id>
		<navigation-case>
			<from-outcome>login</from-outcome>
			<to-view-id>/jsp/login.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/jsp/index.jsp</from-view-id>
		<navigation-case>
			<from-outcome>index</from-outcome>
			<to-view-id>/jsp/index.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/jsp/login.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/jsp/StudentHome.jsp</from-view-id>
		<navigation-case>
			<from-outcome>takeTest</from-outcome>
			<to-view-id>/jsp/TakeTest.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>feedback</from-outcome>
			<to-view-id>/jsp/FeedBack.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/jsp/Admin.jsp</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/jsp/AdminSuccess.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>error</from-outcome>
			<to-view-id>/jsp/Admin.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/jsp/TeacherHome.jsp</from-view-id>
		<navigation-case>
			<from-outcome>viewuploadRoaster</from-outcome>
			<to-view-id>/jsp/ViewUploadRoaster.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>viewuploadTest</from-outcome>
			<to-view-id>/jsp/ViewUploadTest.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>uploadQuestions</from-outcome>
			<to-view-id>/jsp/UploadQuestions.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>statistical</from-outcome>
			<to-view-id>/jsp/StatisticalAnalysis.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>graphical</from-outcome>
			<to-view-id>/jsp/ChartAnalysis.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/jsp/TakeTest.jsp</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/jsp/StudentHome.jsp</to-view-id>
		</navigation-case>

	</navigation-rule>
	<managed-bean>
		<managed-bean-name>LoginUserBean</managed-bean-name>
		<managed-bean-class>edu.uic.model.bean.LoginUserBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>DBBean</managed-bean-name>
		<managed-bean-class>edu.uic.model.bean.DBBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>DbAdminBean</managed-bean-name>
		<managed-bean-class>edu.uic.dao.DbAdminBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>TestBean</managed-bean-name>
		<managed-bean-class>edu.uic.model.bean.TestBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>QuestionBean</managed-bean-name>
		<managed-bean-class>edu.uic.model.bean.QuestionBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>

	</managed-bean>
	<managed-bean>
		<managed-bean-name>RoasterBean</managed-bean-name>
		<managed-bean-class>edu.uic.model.bean.RoasterBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>CourseBean</managed-bean-name>
		<managed-bean-class>edu.uic.model.bean.CourseBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>NavigationBean</managed-bean-name>
		<managed-bean-class>edu.uic.model.bean.NavigationBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>StatisticalBean</managed-bean-name>
		<managed-bean-class>edu.uic.model.bean.StatisticalBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>ChartBean</managed-bean-name>
		<managed-bean-class>edu.uic.model.bean.ChartBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
</faces-config>